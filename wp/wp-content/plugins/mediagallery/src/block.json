{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/mediagallery",
	"version": "0.1.0",
	"title": "Media Gallery",
	"category": "text",
	"description": "A collection of media assets to support a pop-up gallery",
	"keywords": [ "ffa", "agexplorer", "media", "video", "gallery", "item" ],
	"supports": {
		"html": false
	},
	"textdomain": "mg-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"_cover": {
			"type": "string",
			"default": ""
		},
		"columns": {
			"type": "number",
			"default": 2
		}
	},
	"example": {
		"attributes": {
			"_cover": "https://placehold.co/550x550/000000/FFF"
		}
	}
}
