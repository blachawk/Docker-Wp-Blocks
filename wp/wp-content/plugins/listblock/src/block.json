{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "blachawk-blocks/list-block",
	"version": "0.1.0",
	"title": "List Block",
	"category": "text",
	"description": "A custom list block.  This will display in the preview panel.",
	"keywords": [ "blachawk", "lists", "list", "item" ],
	"supports": {
		"html": false
	},
	"textdomain": "bh-l",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"listOrdered": {
			"type": "boolean",
			"default": false
		},
		"_cover": {
			"type": "string",
			"default": ""
		}
	},
	"example": {
		"attributes": {
			"_cover": "https://placehold.co/550x550/000000/FFF"
		}
	}
}
